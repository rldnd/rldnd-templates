{
  "name": "<%= projectName %>",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "APP_ENV=development vite --mode dev",
    "stage": "APP_ENV=staging vite --mode stage",
    "prod": "APP_ENV=production vite --mode prod",
    "build:dev": "tsc && APP_ENV=development vite build --mode dev && dist-size ./build",
    "build:stage": "tsc && APP_ENV=staging vite build --mode stage && dist-size ./build",
    "build:prod": "tsc && APP_ENV=production vite build --mode production && dist-size ./build",
    "preview": "vite preview",
    "prepare": "husky install",
    "prettier:fix": "prettier **/*.ts **/*.tsx --write",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./src",
    "stylelint:fix": "stylelint **/*.scss --fix",
    "pre-commit": "yarn lint-staged"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "axios": "^1.2.3",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "env-cmd": "^10.1.0",
    "events": "^3.3.0",
    "flatpickr": "^4.6.13",
    "i18n-iso-countries": "^7.5.0",
    "i18next": "^22.4.9",
    "i18next-browser-languagedetector": "^7.0.1",
    "lodash-es": "^4.17.21",
    "path": "^0.12.7",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-flatpickr": "^3.10.13",
    "react-hook-form": "^7.42.1",
    "react-i18next": "^12.1.4",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.7.0",
    "react-transition-group": "^4.4.5",
    "recoil": "^0.7.6",
    "vite-plugin-svgr": "^2.4.0",
    "yup": "^0.32.11"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "yarn prettier:fix",
      "yarn lint:fix"
    ],
    "src/**/*.scss": [
      "yarn stylelint:fix"
    ]
  },
  "devDependencies": {
    "@types/events": "^3.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-flatpickr": "^3.8.8",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "@vitejs/plugin-react": "^3.0.0",
    "dist-size": "^0.4.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": ">=10",
    "prettier": "^2.8.3",
    "sass": "^1.57.1",
    "stylelint": "^14.16.1",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-declaration-strict-value": "^1.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-checker": "^0.5.5",
    "vite-plugin-env-compatible": "^1.1.1"
  }
}
